{
    "name": "mileschou/toggle-processor",
    "license": "MIT",
    "description": "The processors implements for feature toggle library.",
    "keywords": [
        "feature",
        "toggle"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-json": "*",
        "mileschou/toggle-simplify": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4 | ^5 | ^6",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "MilesChou\\Toggle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
